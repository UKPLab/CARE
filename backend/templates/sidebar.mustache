<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8">
    </head>
    <body>
        <script>
            localStorage.setItem("hypothesis.oauth.localhost.token", JSON.stringify({{{ oauth }}}))
        </script>
        <hypothesis-app></hypothesis-app>

        <script class="js-hypothesis-config" type="application/json">
             {"apiUrl":"{{{ appUrl }}}/api/",
              "assetRoot":"/hypothesis/",
              "authDomain":"localhost",
              "serviceUrl":"{{{ appUrl }}}/",
              "release":"1.1023.0.0",
              "appType":"chrome-extension",
              "oauthClientId": "{{{ oauthClientId }}}"
             }
        </script>
        <script src="/hypothesis/build/boot.js"></script>

    </body>
</html>