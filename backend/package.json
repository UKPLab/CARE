{
  "name": "PEERBackend",
  "version": "0.0.1",
  "scripts": {
    "start": "node index.js",
    "db_create": "cd db && npx sequelize-cli db:create",
    "db_migrate": "cd db && npx sequelize-cli db:migrate",
    "db_migrate:reset": "cd db && npm run db_create || echo \"IGNORING ERROR\" && npx sequelize-cli db:migrate:undo:all && npm run db_migrate",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000",
    "pretest": "cross-env NODE_ENV=test npm run db_migrate:reset"
  },
  "dependencies": {
    "argparse": "^2.0.1",
    "axios": "^0.27.2",
    "axios-oauth-client": "^1.5.0",
    "bcrypt": "5.0.0",
    "body-parser": "^1.20.0",
    "connect-ensure-login": "^0.1.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-fileupload": "^1.4.0",
    "express-rate-limit": "^6.4.0",
    "express-session": "^1.17.3",
    "form-data": "^4.0.0",
    "mustache-express": "^1.3.2",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg-promise": "^10.11.1",
    "sequelize": "^6.21.3",
    "sequelize-cli": "^6.4.1",
    "session-file-store": "^1.5.0",
    "socket.io": "=4.5.1",
    "socket.io-client": "=4.5.1",
    "supertest": "^6.2.4",
    "uuid": "^8.3.2",
    "winston": "^3.8.1",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.0.1"
  }
}
