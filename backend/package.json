{
  "name": "CAREBackend",
  "version": "1.1.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "db_create": "cd db && npx sequelize-cli db:create",
    "db_migrate": "cd db && npx sequelize-cli db:migrate",
    "db_migrate:reset": "cd db && npx sequelize-cli db:drop && npm run db_create && npm run db_migrate",
    "db_migrate:reset2": "cd db && npm run db_create || echo \"IGNORING ERROR\" && npx sequelize-cli db:migrate:undo:all && npm run db_migrate",
    "test": "cross-env NODE_ENV=test DISABLE_LOGGING=false jest --silent=false --forceExit --testTimeout=10000 --runInBand --collect-coverage -- test.js",
    "test_rpc": "cross-env NODE_ENV=test DISABLE_LOGGING=false jest --silent=false --forceExit --testTimeout=10000  --runInBand  --testPathPattern=tests/rpcs -- ",
    "test_doh": "cross-env NODE_ENV=test jest  --detectOpenHandles --testTimeout=10000",
    "pretest": "cross-env NODE_ENV=test npm run db_migrate:reset"
  },
  "dependencies": {
    "argparse": "^2.0.1",
    "axios": "^0.27.2",
    "axios-oauth-client": "^1.5.0",
    "bcrypt": "5.0.0",
    "body-parser": "^1.20.0",
    "connect-ensure-login": "^0.1.1",
    "connect-session-sequelize": "^7.1.5",
    "cors": "^2.8.5",
    "dayjs": "^1.11.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-fileupload": "^1.4.0",
    "express-rate-limit": "^6.4.0",
    "express-session": "^1.17.3",
    "form-data": "^4.0.0",
    "js-yaml": "^4.1.0",
    "mustache-express": "^1.3.2",
    "objects-to-csv": "^1.3.6",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg-promise": "^10.11.1",
    "editor-delta-conversion": "file:../utils/modules/editor-delta-conversion",
    "quill-delta": "^5.1.0",
    "jszip": "^3.10.1",
    "ajv": "^8.12.0",
    "micromatch": "^4.0.5",
    "request": "^2.88.2",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.4.1",
    "sequelize-mock": "^0.10.2",
    "session-file-store": "^1.5.0",
    "socket.io": "=4.5.1",
    "socket.io-client": "=4.5.1",
    "string-replace-middleware": "^1.0.2",
    "supertest": "^6.2.4",
    "uuid": "^8.3.2",
    "winston": "^3.8.1",
    "winston-transport": "^4.5.0",
    "yauzl": "^3.2.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.0.1"
  }
}
